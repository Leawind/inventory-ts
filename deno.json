{
	"name": "@leawind/inventory",
	"version": "0.14.0",
	"license": "MIT",
	"exports": {
		"./algorithm": "./src/algorithm/index.ts",
		"./baffer": "./src/baffer/index.ts",
		"./call_stack": "./src/call-stack/index.ts",
		"./comap": "./src/comap.ts",
		"./communication": "./src/communication/index.ts",
		"./csv": "./src/csv/index.ts",
		"./deferred": "./src/deferred/index.ts",
		"./env_manager": "./src/env-manager/index.ts",
		"./fs": "./src/fs/index.ts",
		"./func_type": "./src/func-type/index.ts",
		"./IdSequencer": "./src/IdSequencer/index.ts",
		"./imagine": "./src/imagine/index.ts",
		"./list_iterator": "./src/list-iterator/index.ts",
		"./lock": "./src/lock/index.ts",
		"./misc": "./src/misc/index.ts",
		"./monolist": "./src/monolist/index.ts",
		"./options": "./src/options/index.ts",
		"./static_resource": "./src/static-resource/index.ts",
		"./test_utils": "./src/test-utils.ts",
		"./throttled_action": "./src/throttled-action/index.ts",
		"./tree": "./src/tree.ts",
		"./tstr": "./src/tstr/index.ts",
		"./types": "./src/types.ts",
		"./waited": "./src/waited/index.ts",
		"./wash": "./src/wash/index.ts",
		"./xtype": "./src/xtype/index.ts",
		".": "./src/index.ts"
	},
	"tasks": {
		"dev": "deno test --watch --parallel -R",
		"test": "deno test --parallel -A",
		"gen-index": "deno -A ./scripts/gen_index.ts",
		"build-npm": "deno run -A scripts/build_npm.ts",
		"prepare": "deno task gen-index && deno fmt && deno lint && deno task test && deno publish --dry-run",
		"prepare-npm": "deno task gen-index && deno fmt && deno lint && deno task test && deno task build-npm && cd npm && npm publish --dry-run",
		"install-til": "deno install -g -A -n til ./src/bin/til.ts"
	},
	"test": {
		"include": [
			"src/**/*.test.ts",
			"test/**/*.test.ts"
		]
	},
	"publish": {
		"exclude": [
			"examples/",
			"**/*.test.ts",
			"scripts",
			".github/",
			".vscode/"
		]
	},
	"imports": {
		"@deno/dnt": "jsr:@deno/dnt@^0.41.3",
		"@leawind/delegate": "jsr:@leawind/delegate@^0.6.0",
		"@std": "jsr:@std",
		"@/": "./src/"
	},
	"lint": {
		"include": [
			"src/**/*.ts"
		]
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 120,
		"indentWidth": 4,
		"semiColons": true,
		"singleQuote": true,
		"proseWrap": "preserve",
		"include": [
			"README.md",
			"deno.json",
			"scripts",
			"src/",
			"test/"
		],
		"exclude": []
	}
}
